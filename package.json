{
  "name": "xke-rxjs",
  "version": "1.0.0",
  "description": "RXJS show-case",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch & npm run livereload & npm run static",
    "watch": "watchify app/app.js -o app/bundle.js -d -t [ babelify --sourceMapRelative . --presets [ es2015 ] ]",
    "livereload": "livereloadify app",
    "static": "static app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flegall/xke-rxjs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flegall/xke-rxjs/issues"
  },
  "readme": "https://github.com/flegall/xke-rxjs#readme",
  "homepage": "https://github.com/flegall/xke-rxjs#readme",
  "dependencies": {
    "immutable": "3.7.5",
    "rx": "4.0.7",
    "yolk": "0.9.0"
  },
  "babel": {
    "sourceMaps": "inline",
    "plugins": [
      [
        "babel-plugin-transform-react-jsx",
        {
          "pragma": "Yolk.h"
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babelify": "7.2.0",
    "browserify": "12.0.1",
    "livereloadify": "2.0.0",
    "node-static": "0.7.7",
    "watchify": "3.6.1"
  }
}
