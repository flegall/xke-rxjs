{
  "name": "xke-rxjs",
  "version": "1.0.0",
  "description": "RXJS show-case",
  "main": "index.js",
  "scripts": {
    "start": "browserify src/app.js -o application.js -t [ babelify --presets [ es2015 ] ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flegall/xke-rxjs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flegall/xke-rxjs/issues"
  },
  "homepage": "https://github.com/flegall/xke-rxjs#readme",
  "dependencies": {
    "immutable": "3.7.5",
    "rx": "4.0.7",
    "yolk": "0.9.0"
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-transform-react-jsx",
        {
          "pragma": "Yolk.h"
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babelify": "7.2.0",
    "browserify": "12.0.1"
  }
}
